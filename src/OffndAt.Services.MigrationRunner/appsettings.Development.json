{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "PersistenceSettings": {
    "ConnectionString": "Host=localhost;Username=offndat;Password=offndat;Database=offndat"
  },
  "ApplicationSettings": {
    "AppName": "offnd-at-migration-runner",
    "Environment": "Development",
    "BaseDomain": "localhost",
    "UseHttps": false
  },
  "CacheSettings": {
    "LongTtl": "01:00:00",
    "ShortTtl": "00:01:00"
  },
  "TelemetrySettings": {
    "Enabled": true,
    "ExporterEndpoint": "http://localhost:4317/"
  }
}
